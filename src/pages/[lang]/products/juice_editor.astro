---
import JuiceEditorPage from '@/components/JuiceEditor/JuiceEditorPage.vue';
import AppLayout from '@/layouts/AppLayout.astro';
interface Props {
  lang: string;
}

const { lang } = Astro.params as Props;

export const prerender = true;
export const getStaticPaths = async () => {
  return [{ params: { lang: 'zh-cn' } }, { params: { lang: 'en' } }];
};
---

<AppLayout
  title="Juice Editor"
  description="Juice Editor is a tool for creating and managing content for your website.">
  <JuiceEditorPage lang={lang} />
</AppLayout>
