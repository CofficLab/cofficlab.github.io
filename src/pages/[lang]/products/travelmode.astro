---
import TravelModePage from '@/components/TravelMode/TravelModePage.vue';
import AppLayout from '@/layouts/AppLayout.astro';
interface Props {
  lang: string;
}

const { lang } = Astro.params as Props;

export const prerender = true;
export const getStaticPaths = async () => {
  return [{ params: { lang: 'zh-cn' } }, { params: { lang: 'en' } }];
};
---

<AppLayout
  title="TravelMode"
  description="TravelMode is a tool for creating and managing content for your website.">
  <TravelModePage lang={lang} />
</AppLayout>
