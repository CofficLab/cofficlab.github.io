---
import StarlightPage from '@astrojs/starlight/components/StarlightPage.astro';
import JuiceEditorPage from '../components/JuiceEditor/JuiceEditorPage.vue';
const lang = Astro.currentLocale ?? 'en';
---

<StarlightPage
  frontmatter={{
    title: 'JuiceEditor',
    description: '简洁编辑器',
    template: 'splash',
    hero: {
      title: 'JuiceEditor',
      image: {
        file: {
          src: '/images/coffic/logo4.png',
          width: 8,
          height: 7,
          format: 'png',
        },
      },
      actions: [
        {
          text: 'Get Started',
          link: '/juiceeditor/guide/introduce',
        },
      ],
    },
  }}
  hasSidebar={false}
  headings={[]}>
  <div class="not-content">
    <JuiceEditorPage lang={lang} />
  </div>
</StarlightPage>
